               RowBox[{"Background", " ", "\[Rule]", " ", 
                RowBox[{"GrayLevel", "[", ".95", "]"}]}], ",", 
               RowBox[{"FontSize", " ", "\[Rule]", " ", "10"}]}], "]"}], ",", 
             RowBox[{"{", 
              RowBox[{
               RowBox[{
                RowBox[{"aa", "[", "i", "]"}], "=", 
                RowBox[{
                 RowBox[{"crvlbl", "[", 
