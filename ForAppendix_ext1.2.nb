       RowBox[{"(", 
        RowBox[{
         RowBox[{"2", " ", "l"}], "+", "1"}], ")"}], 
       RowBox[{"l", " ", 
        RowBox[{"(", 
         RowBox[{"l", "+", "1"}], ")"}]}]], ")"}], 
     RowBox[{"(", 
      RowBox[{
       RowBox[{
        RowBox[{"an", "[", 
         RowBox[{"l", ",", "a"}], "]"}], 
        RowBox[{"pi", "[", 
         RowBox[{"l", ",", "\[Theta]"}], "]"}]}], "+", 
       RowBox[{
        RowBox[{"bn", "[", 
         RowBox[{"l", ",", "a"}], "]"}], "  ", 
