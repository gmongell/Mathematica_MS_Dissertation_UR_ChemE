   RowBox[{"CScat", "[", "a", "]"}], 
   RowBox[{"\[Pi]", " ", 
    SuperscriptBox["a", "2"]}]]}], "\n", 
 RowBox[{
  RowBox[{"QAbs", "[", "a_", "]"}], ":=", " ", 
  FractionBox[
   RowBox[{"CAbs", "[", "a", "]"}], 
   RowBox[{"\[Pi]", " ", 
    SuperscriptBox["a", "2"]}]]}], "\n", 
 RowBox[{
  RowBox[{"QExt", "[", "a_", "]"}], ":=", 
  FractionBox[
   RowBox[{"CExt", "[", "a", "]"}], 
   RowBox[{"\[Pi]", " ", 
    SuperscriptBox["a", "2"]}]]}]}], "Input"],

